<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox spacing="5.0" alignment="CENTER" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="AuthWindowController"
      prefHeight="390.0" prefWidth="700.0" minHeight="390.0" minWidth="290.0">

    <ImageView fitHeight="200.0" fitWidth="200.0" >
        <Image url="@/Images/github.png"/>
    </ImageView>

    <TextField fx:id="login" maxWidth="200.0" prefWidth="200.0" promptText="Login"/>

    <PasswordField fx:id="password" maxWidth="200.0" prefWidth="200.0" promptText="Password" />

    <Button fx:id="submit" text="Login" onAction="#click" prefWidth="100.0"/>

</VBox>
